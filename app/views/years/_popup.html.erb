<div id="popup" class="x-hidden">
  <div class="x-window-header"></div>
  <div id="popup_main" class="term-popup">
    <%= form_tag({ :action => "create_term" }, { :remote => true, :method => "POST", :id => "term_form" }) do %>
      <table>
        <tr>
          <td>Начало</td>
          <td><%= extjs_date_field_control :term, :start_date %></td>
        </tr>
        <tr>
          <td>Конец</td>
          <td><%= extjs_date_field_control :term, :end_date %></td>
        </tr>
      </table>
    <% end %>
  </div>
</div>